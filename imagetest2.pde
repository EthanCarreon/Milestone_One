void keyPressed() {
  if (keyCode==65) {
    left = true;
  } else if (keyCode==87) {
    up = true;
  } else if (keyCode==68) {
    right = true;
  } else if (keyCode==83) {
    down=true;
  }
}
void keyReleased() {
  if (keyCode==65) {
    left = false;
  } else if (keyCode==87) {
    up = false;
  } else if (keyCode==68) {
    right = false;
  } else if (keyCode==83) {
    down=false;
  }
}

